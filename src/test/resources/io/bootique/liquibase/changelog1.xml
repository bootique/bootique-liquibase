<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <changeSet id="create_table_a" author="unknown">
        <createTable tableName="A">
            <column name="ID" type="java.sql.Types.VARCHAR(32)">
                <constraints nullable="false"/>
            </column>
            <column name="NAME" type="java.sql.Types.VARCHAR(32)"></column>
            <column name="ORDER" type="java.sql.Types.INTEGER"></column>
        </createTable>
    </changeSet>

    <changeSet id="add_pk_a" author="unknown">
        <addPrimaryKey tableName="A" columnNames="ID"/>
    </changeSet>

    <changeSet id="insert_a_prod" author="unknown" context="prod">
        <insert tableName="A">
            <column name="ID" value="1"></column>
            <column name="NAME" value="Prod"></column>
            <column name="ORDER" valueNumeric="1"/>
        </insert>
    </changeSet>
    <changeSet id="insert_a_test" author="unknown" context="test">
        <insert tableName="A">
            <column name="ID" value="2"></column>
            <column name="NAME" value="Test"></column>
            <column name="ORDER" valueNumeric="2"/>
        </insert>
    </changeSet>
    <changeSet id="insert_a_common" author="unknown">
        <insert tableName="A">
            <column name="ID" value="3"></column>
            <column name="NAME" value="Common"></column>
            <column name="ORDER" valueNumeric="3"/>
        </insert>
    </changeSet>

</databaseChangeLog>